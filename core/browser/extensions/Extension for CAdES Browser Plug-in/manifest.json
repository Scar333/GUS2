{
   "action": {
      "default_icon": {
         "128": "icons/icon128.png",
         "16": "icons/icon16.png",
         "48": "icons/icon48.png"
      },
      "default_popup": "popup.html"
   },
   "author": "CadesCompany Ltd",
   "background": {
      "service_worker": "background.js"
   },
   "content_scripts": [ {
      "all_frames": true,
      "exclude_matches": [ "*://*.icloud.com/*" ],
      "js": [ "content.js" ],
      "matches": [ "\u003Call_urls>" ]
   } ],
   "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self';"
   },
   "description": "Расширение позволяет использовать ЭЦП Browser plug-in в браузере. Для работы требует установленного ЭЦП Browser plug-in.",
   "homepage_url": "https://www.cadescompany.ru/cadesplugin",
   "icons": {
      "128": "icons/icon128.png",
      "16": "icons/icon16.png",
      "48": "icons/icon48.png"
   },
   "key": "",
   "manifest_version": 3,
   "minimum_chrome_version": "105",
   "name": "Extension for CAdES Browser Plug-in",
   "permissions": [ "nativeMessaging" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.3.12",
   "web_accessible_resources": [ {
      "matches": [ "\u003Call_urls>" ],
      "resources": [ "nmcades_plugin_api.js", "trusted_sites.html", "icons/icon16.png" ]
   } ]
}
